# Engine root directory (must exist)
ENGINE_ROOT=$HOME/engine-root

# Path to WildFly oVirt overlay modules directory
WILDFLY_OVIRT_OVERLAY=/usr/share/ovirt-engine-wildfly-overlay/modules

